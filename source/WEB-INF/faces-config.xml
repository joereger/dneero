<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd" [
<!ENTITY managed-beans_gen          SYSTEM "managed-beans_gen.xml">
<!ENTITY sample-navigation-rules    SYSTEM "sample-navigation-rules.xml">
 <!ENTITY converters_gen    SYSTEM "converters_gen.xml">
]>

<faces-config>

    <!--
    <application>
        <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
      </application>
      -->


    <application>
        <locale-config>
          <default-locale>en_US</default-locale>
        </locale-config>
    </application>


    <application>
        <default-render-kit-id>oracle.adf.core</default-render-kit-id>
    </application>

    <managed-bean>
        <managed-bean-name>userSession</managed-bean-name>
        <managed-bean-class>com.dneero.session.UserSession</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>registration</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.Registration</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


    <managed-bean>
        <managed-bean-name>userList</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.UserList</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>bloggerOfferList</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.BloggerOfferList</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>bloggerEarnings</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.BloggerEarnings</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>bloggerOfferDetail</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.BloggerOfferDetail</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>offer</property-name>
            <value>#{offer}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>newOffer</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.NewOffer</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>genders</managed-bean-name>
        <managed-bean-class>java.util.HashMap</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
        <map-entries>
            <map-entry>
                <key>Male</key>
                <value>0</value>
            </map-entry>
            <map-entry>
                <key>Female</key>
                <value>1</value>
            </map-entry>
        </map-entries>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>ethnicities</managed-bean-name>
        <managed-bean-class>java.util.HashMap</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
        <map-entries>
            <map-entry>
                <key>Not Specified</key>
                <value>0</value>
            </map-entry>
            <map-entry>
                <key>White, Not Hispanic</key>
                <value>1</value>
            </map-entry>
            <map-entry>
                <key>Black, Not Hispanic</key>
                <value>2</value>
            </map-entry>
            <map-entry>
                <key>Hispanic</key>
                <value>3</value>
            </map-entry>
            <map-entry>
                <key>Asian</key>
                <value>4</value>
            </map-entry>
            <map-entry>
                <key>American Indian</key>
                <value>5</value>
            </map-entry>
        </map-entries>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>maritalstatuses</managed-bean-name>
        <managed-bean-class>java.util.HashMap</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
        <map-entries>
            <map-entry>
                <key>Single</key>
                <value>0</value>
            </map-entry>
            <map-entry>
                <key>Married</key>
                <value>1</value>
            </map-entry>
            <map-entry>
                <key>Divorced</key>
                <value>2</value>
            </map-entry>
            <map-entry>
                <key>Engaged</key>
                <value>3</value>
            </map-entry>
        </map-entries>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>incomes</managed-bean-name>
        <managed-bean-class>java.util.HashMap</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
        <map-entries>
            <map-entry>
                <key>0 - 10000</key>
                <value>0</value>
            </map-entry>
            <map-entry>
                <key>10001 - 20000</key>
                <value>1</value>
            </map-entry>
            <map-entry>
                <key>20001 - 30000</key>
                <value>2</value>
            </map-entry>
            <map-entry>
                <key>30001 - 40000</key>
                <value>3</value>
            </map-entry>
            <map-entry>
                <key>40001 - 50000</key>
                <value>4</value>
            </map-entry>
            <map-entry>
                <key>50001 - 60000</key>
                <value>5</value>
            </map-entry>
            <map-entry>
                <key>60001 - 70000</key>
                <value>6</value>
            </map-entry>
            <map-entry>
                <key>70001 - 80000</key>
                <value>7</value>
            </map-entry>
            <map-entry>
                <key>80001 - 90000</key>
                <value>8</value>
            </map-entry>
            <map-entry>
                <key>90001 - 100000</key>
                <value>9</value>
            </map-entry>
            <map-entry>
                <key>100001 - 120000</key>
                <value>10</value>
            </map-entry>
            <map-entry>
                <key>120001 - 150000</key>
                <value>11</value>
            </map-entry>
            <map-entry>
                <key>150001 - 200000</key>
                <value>12</value>
            </map-entry>
            <map-entry>
                <key>200000 and Above</key>
                <value>13</value>
            </map-entry>
        </map-entries>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>educationlevels</managed-bean-name>
        <managed-bean-class>java.util.HashMap</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
        <map-entries>
            <map-entry>
                <key>None</key>
                <value>0</value>
            </map-entry>
            <map-entry>
                <key>High School</key>
                <value>1</value>
            </map-entry>
            <map-entry>
                <key>Some College</key>
                <value>2</value>
            </map-entry>
            <map-entry>
                <key>College Degree</key>
                <value>3</value>
            </map-entry>
            <map-entry>
                <key>Advanced Degree</key>
                <value>4</value>
            </map-entry>
        </map-entries>
    </managed-bean>
    

    <managed-bean>
        <managed-bean-name>researcherOfferList</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.ResearcherOfferList</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>





    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registration</from-outcome>
            <to-view-id>/registration.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userlist</from-outcome>
            <to-view-id>/userlist.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>researcherhome</from-outcome>
            <to-view-id>/researcher/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>newoffer</from-outcome>
            <to-view-id>/researcher/newoffer.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerearnings</from-outcome>
            <to-view-id>/blogger/earnings.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerhome</from-outcome>
            <to-view-id>/blogger/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerofferlist</from-outcome>
            <to-view-id>/blogger/bloggerofferlist.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerofferdetail</from-outcome>
            <to-view-id>/blogger/bloggerofferdetail.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>researcherofferlist</from-outcome>
            <to-view-id>/researcher/researcherofferlist.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/blogger/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
		<from-view-id>/blogger/bloggerofferlist.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>beginview_success</from-outcome>
			<to-view-id>/blogger/bloggerofferdetail.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
        <from-view-id>/registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/registration-success.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/researcher/newoffer.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/researcher/researcherofferlist.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>


</faces-config>