<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd" [
<!ENTITY managed-beans_gen          SYSTEM "managed-beans_gen.xml">
<!ENTITY sample-navigation-rules    SYSTEM "sample-navigation-rules.xml">
 <!ENTITY converters_gen    SYSTEM "converters_gen.xml">
]>

<faces-config>

    <!--
    <application>
        <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
      </application>
      -->


    <application>
        <locale-config>
          <default-locale>en_US</default-locale>
        </locale-config>
    </application>

    <application>
        <default-render-kit-id>oracle.adf.core</default-render-kit-id>
    </application>

    <component>
        <component-type>com.dneero.ui.RoundedCornerBox</component-type>
        <component-class>com.dneero.ui.RoundedCornerBox</component-class>
    </component>

    <component>
        <component-type>com.dneero.session.Authorization</component-type>
        <component-class>com.dneero.session.Authorization</component-class>
    </component>

    <managed-bean>
        <managed-bean-name>userSession</managed-bean-name>
        <managed-bean-class>com.dneero.session.UserSession</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>registration</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.Registration</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>accountSettings</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.AccountSettings</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>changePassword</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.ChangePassword</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


    <managed-bean>
        <managed-bean-name>userList</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.UserList</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>bloggerOfferList</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.BloggerOfferList</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>bloggerDetails</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.BloggerDetails</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>researcherDetails</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.ResearcherDetails</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>sysadminOfferList</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.SystemadminOfferList</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>bloggerEarnings</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.BloggerEarnings</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>bloggerOfferDetail</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.BloggerOfferDetail</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>offer</property-name>
            <value>#{offer}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>newOffer</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.NewOffer</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.Login</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


    <managed-bean>
        <managed-bean-name>researcherOfferList</managed-bean-name>
        <managed-bean-class>com.dneero.formbeans.ResearcherOfferList</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>userSession</managed-bean-name>
        <managed-bean-class>com.dneero.session.UserSession</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>





    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registration</from-outcome>
            <to-view-id>/registration.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>accountsettings</from-outcome>
            <to-view-id>/account/accountsettings.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>changepassword</from-outcome>
            <to-view-id>/account/changepassword.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout_success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userlist</from-outcome>
            <to-view-id>/sysadmin/userlist.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>sysadminhome</from-outcome>
            <to-view-id>/sysadmin/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>sysadminofferlist</from-outcome>
            <to-view-id>/sysadmin/sysadminofferlist.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>researcherhome</from-outcome>
            <to-view-id>/researcher/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>newoffer</from-outcome>
            <to-view-id>/researcher/newoffer.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerearnings</from-outcome>
            <to-view-id>/blogger/earnings.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerhome</from-outcome>
            <to-view-id>/blogger/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerdetails</from-outcome>
            <to-view-id>/blogger/bloggerdetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerofferlist</from-outcome>
            <to-view-id>/blogger/bloggerofferlist.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bloggerofferdetail</from-outcome>
            <to-view-id>/blogger/bloggerofferdetail.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>researcherofferlist</from-outcome>
            <to-view-id>/researcher/researcherofferlist.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>researcherdetails</from-outcome>
            <to-view-id>/researcher/researcherdetails.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/blogger/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
		<from-view-id>/blogger/bloggerofferlist.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>beginview_success</from-outcome>
			<to-view-id>/blogger/bloggerofferdetail.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
        <from-view-id>/registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/registration-success.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/blogger/bloggerdetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/blogger/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>success_newblogger</from-outcome>
            <to-view-id>/blogger/welcomenewblogger.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/researcher/researcherdetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/researcher/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>success_newresearcher</from-outcome>
            <to-view-id>/researcher/welcomenewresearcher.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/researcher/newoffer.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/researcher/researcherofferlist.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/account/accountsettings.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/account/changepassword.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>






</faces-config>